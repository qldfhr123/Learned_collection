@charset "UTF-8";
@import url('main.css'); 
@import url('nav.css');
@import url('sub.css'); 
@font-face {
    font-family: 'GmarketSansMedium';
    src: url('https://cdn.jsdelivr.net/gh/projectnoonnu/noonfonts_2001@1.1/GmarketSansMedium.woff') format('woff');
    font-weight: normal;
    font-style: normal;
}
html,body{word-break:keep-all; font-size:16px; color:#2f2c28; font-weight:300;}
body,h1,h2,h3,h4,h5,h6,p,table,tr,th,td,ul,li,ol{padding:0; margin:0; font-family: 'Noto Sans KR';}
table{border-collapse:collapse; border-spacing:0;}
a{color:#454545; text-decoration:none;}
img{border:0;}
ul,li,ol{list-style:none;}

