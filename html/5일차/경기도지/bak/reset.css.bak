@charset "utf-8";
@import url('main.css');
@import url('sub.css');
@import url('main.css');
  @import url('https://fonts.googleapis.com/css2?family=Nanum+Gothic:wght@700;800&family=Noto+Sans+KR:wght@100;300;400;500;700;900&display=swap');

/*공통 스타일*/
html,body{word-break:keep-all; font-size:16px; color:#2f2c28; font-weight:300;}
/*word-break:keep-all;  글이 길어질때 줄바꿈을 어떻게 할지 지정하는 속성, 단어 사이에서 줄바꿈 금지*/

body,h1,h2,h3,h4,h5,h6,p,table,th,tr,td,p,ul,ol,li{font-family:'Noto Sans KR'; margin:0; padding:0;}
img{border:0;}
a{color:#454545; text-decoration:none;}
table{border-collapse:collapse; border-spacing:0;}
ul,ol,li{list-style:none;}

#wrap{width:630px; margin:50px auto;}


.content .star h2,
.content .star .top p{float:left;}
.content .star .top p:first-child{color:red;}

.content .star ul{ padding-top: 10px; padding-bottom:20px; border-bottom:1px solid #ccc; }
.content .star  ul li{position:relative;  width:33.3%; box-sizing:border-box; -webkit-box-sizing:border-box;}
.content .star ul li  span{margin :0; position:absolute; display:block; width:100px; height:50px; line-height:50px; background: #ff00ff; font-size:10px; color:#fff; font-weight:bold; text-align:center;}

