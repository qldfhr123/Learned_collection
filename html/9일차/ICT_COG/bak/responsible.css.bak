@charset "utf-8";


/*공통*/
@media screen and (max-width:770px){
   body > header h1 img{width:120px; padding-top:15px;}
}

@media screen and (max-width:730px){
   .content .hot ul {display: block;padding-left:0px;}
.content .hot{margin-left:25px;}
.content .people a p {top:60px;}
}

@media screen and (max-width:700px){
 .visual .inner .bar {position:absolute; left: 85px; bottom:170px; width:450px; height:6px; border:1px solid; background:rgba(0,0,0,0.2);}
.visual .inner .bar .green{position:absolute; left: -1.2px; bottom:-1px; width:150px; height:6px; border:1px solid; background:#a2f011}
.content .inner_mu a {padding-right: 40px;}
}

@media screen and (max-width:690px){
.content .hot ul li a img {width:80%;}
   .content .hot ul {display: block;padding-left:20px;}
.content .hot{margin-left:25px;}
.content .people a p {top:60px;}
.content .hot ul li a p {
    margin-top: 20px;
    font-family: 'GmarketSansMedium';
    font-size: 18px;
    font-weight: 300;
    color: #000;
}
.content .hot  ul li:nth-child(2){padding-left:15px;}
.content .hot  ul li:nth-child(3){padding-right:15px;}
}

@media screen and (max-width:688px){
.content .hot ul li a img {width:80%;}
   .content .hot ul {display: block;padding-left:20px;}
.content .hot{margin-left:25px;}
.content .people a p {top:60px;}

.content .hot  ul li:nth-child(2){padding-left:10px;}
.content .hot  ul li:nth-child(3){padding-right:10px;}
.content .inner_tag ul li {
    display: inline-block;
    width: 100px;}
	.content .inner_tag ul li:nth-child(n+4):nth-child(-n+6) {
    width: 140px;
}
.content .inner_tag ul li:nth-child(n+7):nth-child(-n+8) {
    width: 125px;
}
}

@media screen and (max-width:688px){
.content .hot ul li a img {width:70%;}
.content .hot  ul li:nth-child(2){padding-left:5px;}
.content .hot  ul li:nth-child(3){padding-right:5px;}
}




@media screen and (max-width:677px){
.content .hot ul li a img {width:70%;}
.content .hot  ul li:nth-child(2){padding-left:5px;}
.content .hot  ul li:nth-child(3){padding-right:5px;}
content .hot ul li a p {
    margin-top: 20px;
    font-family: 'GmarketSansMedium';
    font-size: 14px;
    font-weight: 300;
    color: #000;
}
.content .hot  ul {position:relative;}
.content .hot {height:800px;}
.content .hot  ul li:nth-child(2){position: absolute;right:5px; top: 5px;}
.content .hot  ul li:nth-child(3){position: absolute;left:19px; top: 355px;}
.content .hot  ul li:nth-child(4){position: absolute;right:5px; top:355px;}

}

@media screen and (max-width:653px){
.content .forum ul li p {
    position: absolute;
    left: 240px;
    top: 25px;
}
.content .forum ul li:nth-child(1) span {
    position: absolute;
    left: 240px;
    top: 75px;}
}

@media screen and (max-width:635px){
.content .inner_mu {padding-left: 40px;}
}

@media screen and (max-width:605px){
.content .inner_mu {padding-left: 20px;}
.content .inner_mu a{padding-right: 40px;}
}

@media screen and (max-width:600px){
.content .hot  ul li:nth-child(2){position: absolute;right:-15px; top: -1px;}
.content .hot  ul li:nth-child(3){position: absolute;left:19px; top: 355px;}
.content .hot  ul li:nth-child(4){position: absolute;right:-15px; top:355px;}
}

@media screen and (max-width:597px){
.content .forum ul li span h6 {
    font-size: 18px;
}
.content .forum ul li span {

    font-size: 18px;
}

@media screen and (max-width:581px){
.content .inner_mu{padding-left:13px;}
.content .inner_mu a {width:10%;padding-left:13px;}

 .visual .inner .bar {position:absolute; left: 85px; bottom:170px; width:350px; height:6px; border:1px solid; background:rgba(0,0,0,0.2);}
.visual .inner .bar .green{position:absolute; left: -1.2px; bottom:-1px; width:100px; height:6px; border:1px solid; background:#a2f011}
.content .inner_mu a {padding-right: 40px;}
.content .inner_mu a:nth-child(n+3):(-n+4) p {padding-left:28px;}
}

@media screen and (max-width:581px){
.content .hot  ul li:nth-child(2){position: absolute;right:-45px; top: -1px;}
.content .hot  ul li:nth-child(3){position: absolute;left:19px; top: 355px;}
.content .hot  ul li:nth-child(4){position: absolute;right:-45px; top:355px;}
}

@media screen and (max-width:540px){
.content .inner_mu a {width:10%;}

.content .hot  ul li:nth-child(2){position: absolute;right:-55px; top: -1px;}
.content .hot  ul li:nth-child(3){position: absolute;left:19px; top: 355px;}
.content .hot  ul li:nth-child(4){position: absolute;right:-55px; top:355px;}

}







/*header*/
@media screen and (max-width:480px){
    body > header h1{position:relative; top:10px;}
	body > header h1 img{width:160px;}
	
}
@media screen and (max-width:430px){
    body > header{height:65px;}
	body > header h1{bottom:35px;}
	body > header h1 img{width:130px;display:block;}
	
}
@media screen and (max-width:320px){
    body > header{height:65px;}
	body > header h1{top:700px;}
	body > header h1 img{width:130px;}
}

/*visual*/
@media screen and (max-width:430px){
    .visual .inner > img{display:none;}
    .visual .inner span{display:block;}
}
