@charset "utf-8";

/*공통*/


body > header{position:relative; height:100px; background:#fff;}
body > header h1{padding-left:3%;}
body > header h1 img{width:150px; }/*원래 260px인데 축소*/

header .mu{position:absolute; right:3%; top:35%; z-index:999999999;}
header .mu a{display:block; width:30px; height:30px;}
header .mu a span{display:block; background:#222; width:100%; height:2px; margin-top:7px; margin-bottom:7px;}

/*visual*/
.visual{margin-top:15px;}
.visual .inner img{width:100%;}
.visual .inner {position:relative; }
.visual .inner span {width:572px;height:2px; border:1px solid rgba(0, 0, 0, .5);}
.visual .inner .inner_text  {position:absolute; bottom:190px; left:85px;}
.visual .inner .inner_text  h2{color:#fff; font-weight:500; line-height:35px; font-size:30px;font-family:'GmarketSansMedium'; }
.visual .inner .inner_text  p {padding-top:3px; color:#fff; font-weight:100; line-height:25px; font-size:18px; font-family:'GmarketSanslight'; }
.visual .inner .bar {position:absolute; left: 85px; bottom:170px; width:570px; height:6px; border:1px solid; background:rgba(0,0,0,0.2);}
.visual .inner .bar .green{position:absolute; left: -1.2px; bottom:-1px; width:200px; height:6px; border:1px solid; background:#a2f011}


/*content*/

.content .inner_tag  ul {display: block; padding:40px; 32px; background:#fff; }
.content .inner_tag  ul li{display:inline-block;  width:140px; background:#f7f7f7;border: 1px solid #d8d8d8; height:70px; text-align:center;  line-height : 70px;
border-radius:10px; -webkit-border-radius:10px; -moz-border-radius:10px;
font-size:18px;font-weight:400; color:#4a4a4a;}
.content .inner_tag  ul li:nth-child(n+5){margin-top:15px;}
.content .inner_tag  ul li:nth-child(n+2):nth-child(-n+3) {margin: 0 3px;}
.content .inner_tag  ul li:nth-child(n+6):nth-child(-n+7){margin: 0 3px;}
.content .inner_tag  ul li:nth-child(n+4):nth-child(-n+6) {width:180px;}
.content .inner_tag  ul li:nth-child(n+7):nth-child(-n+8) {width:145px;}

.content .inner_mu {display:block;  height:170px; background:#3f5ac1; padding:35px 5px 35px 65px; display: flex; align-items: center;}
.content .inner_mu a {padding-right:65px;display:inline-block; margin-top:-10px;} 
.content .inner_mu a p {color:#fff; text-align:center;}
.content .inner_mu a:nth-child(4) img {padding-left:8px;}


.content .hot {padding-top:54px; padding-bottom:70px; margin-left:50px; box-sizing:border-box; -webkit-box-sizing:border-box; -moz-box-sizing:border-box; }
.content .hot h3 {font-family:'GmarketSanslight'; font-size:20px;position:relative; padding-left:15px;}
.content .hot h3 span {font-size: 17px; color:#b0bccc; position:absolute; right:80px; top:0;}
.content .hot ul li a img { box-shadow: 3px 3px #c9c9c7, 0em 0 .4em black;  border-radius:10px; }
.content .hot ul {display:block; padding-left:15px;}
.content .hot ul li {padding-top:40px; display:inline-block; padding-top:20px; }
.content .hot  ul li a p{margin-top:20px; font-family:'GmarketSansMedium'; font-size:20px; font-weight:300;color:#000;}
.content .hot  ul li a span {font-size:15px; color:#9ba6bc;}
.content .hot  ul li:nth-child(2){padding-left:38px;}
.content .hot  ul li:nth-child(3){padding-right:38px;}

.content .celebrity { position:relative; }
.content .celebrity h4 {padding:15px 30px; font-size: 50px; font-family:'GmarketSanslight'; font-size:20px;background:#f5f6fa;height:145px; line-height:145px;  }
.content .celebrity h4 span {font-size: 17px; color:#b0bccc; float:right;position:absolute;right:40px;}
.content .people img {width:100%;}
.content .people{position:relative;}
.content  .people a p {background:#3b9fae; color:#fff; border:1px solid #3b9fae; width:85px; height:85px; border-radius:80px; position:absolute; top:230px; left:30px;display: flex; justify-content:center; align-items: center;box-shadow:2px 2px 0 rgba(0,0,0,.3);} 

.content .celebrity_bottom { height:115px; background:#3d9fac;position:relative;padding:40px 35px;
box-shadow:2px 2px 0 rgba(0,0,0,.3);} 
.content .celebrity_bottom h5 {color:#fff; ; padding-top: 40px; font-size:25px;  font-family:'GmarketSansMedium'; font-weight:300;padding-top:10px;  line-height:46px; box-sizing:border-box; -webkit-box-sizing:border-box; -moz-box-sizing:border-box; }
.content .celebrity_bottom h5 p{font-family:'GmarketSanslight'; font-size:20px; }
.content .celebrity_bottom span  {color:#fff; border:1px solid #fff;width:100px; height:46px; position:absolute; right:105px; top:30px;display: flex; justify-content:center; align-items: center; border-radius:40px;}

.content .forum{ padding-top:25px; padding-left: 35px;padding-right: 35px;background:#f5f6fa;}
.content .forum  ul li {position:relative;}
.content .forum  ul li  p{ position:absolute; left:240px; top: 45px;font-size:20px; border-radius:50px;  border:1px solid #bbc2d2; color:#bbc2d2;height:38px; width:83px;display: flex; justify-content:center; align-items: center; }
.content .forum  ul li:nth-child(1),
.content .forum  ul li:nth-child(2){position:relative;overflow:hidden;padding-bottom:45px; padding-top:30px; 
border-bottom:1px solid #bbc2d2;}
.content .forum  ul li:nth-child(3){position:relative;overflow:hidden;padding-bottom:45px; padding-top:30px; }
.content .forum  ul li span { position:absolute; left:240px; top:95px; color:#52596c;font-size:20px;}
.content .forum  ul li span h6 {font-size:25px; font-weight: 700; color:#000;}
.content .forum  ul li:nth-child(3) p{ position:absolute; left:240px; top: 45px;font-size:20px; border-radius:50px;  border:1px solid #bbc2d2; color:#bbc2d2;height:38px; width:170px;display: flex; justify-content:center; align-items: center; }

.content .trip_t{margin:30px 0; height:175px;padding-left: 35px;  background:#fff;}
.content .trip_t > h6{font-size:30px; padding-bottom:15px; padding-top:67px}
.content .trip_t > p {font-size:25px; }

.content .trip  a{position:relative;}
.content .trip  a > img  {width:100%;padding-bottom:20px;}
.content .trip  a > h6 {position:absolute; left:50px; bottom: -180px; color:#fff; font-size:25px;}
.content .trip  a > h6 > p {font-size:20px; font-weight:300;padding-top:10px; line-height:25px;}


footer{height:330px; background:#f5f6fa;}
footer p:nth-child(1) {position:relative; padding:55px 28px; font-size:20px; border-bottom:1px solid #dfe0e4;} 
footer p img:nth-child(1){position:absolute; right:38px; top:57px;}
footer p img:nth-child(2){position:absolute; right:20px; top:57px;}
footer  .f_bottom {padding: 50px 0;}
footer  .f_bottom  span{font-size:25px; font-weight:500;   display:flex; justify-content:center; }
footer  .f_bottom ul{display: block; text-align:center;}

footer  .f_bottom ul li{display:inline-block; text-align:center; padding-top:15px; padding-left:10px;}


